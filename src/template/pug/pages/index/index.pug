					img(src="uploads/pros-spa.jpg" alt="")
				.pros__item
					img(src="uploads/pros-coffee-gift.jpg" alt="")

	.gallery
		.case
			.gallery__head
				.gallery__title.h2.s-primary Наши #[strong  пушистые #[br]гости]
				.gallery__controls
					.gallery__control.control.--prev
						<svg width="20" height="21" viewBox="0 0 20 21" fill="none" xmlns="http://www.w3.org/2000/svg"> <path d="M11.9443 5.64404L16.8003 10.5L11.9443 15.356" stroke="currentColor" stroke-width="1.6" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/> <path d="M3.2002 10.5H16.6642" stroke="currentColor" stroke-width="1.6" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/> </svg>
					.gallery__control.control.--next
						<svg width="20" height="21" viewBox="0 0 20 21" fill="none" xmlns="http://www.w3.org/2000/svg"> <path d="M11.9443 5.64404L16.8003 10.5L11.9443 15.356" stroke="currentColor" stroke-width="1.6" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/> <path d="M3.2002 10.5H16.6642" stroke="currentColor" stroke-width="1.6" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/> </svg>
			.gallery__body
				.gallery__slider.swiper
					.gallery__slider-wrap.swiper-wrapper
						.gallery__slider-item.gallery__item.swiper-slide
							img(src="uploads/gallery-1.jpg" alt="")
						.gallery__slider-item.gallery__item.swiper-slide
							img(src="uploads/gallery-2.jpg" alt="")
						.gallery__slider-item.gallery__item.swiper-slide
							img(src="uploads/gallery-3.jpg" alt="")
						.gallery__slider-item.gallery__item.swiper-slide
							img(src="uploads/gallery-4.jpg" alt="")
						.gallery__slider-item.gallery__item.swiper-slide
							img(src="uploads/gallery-5.jpg" alt="")
