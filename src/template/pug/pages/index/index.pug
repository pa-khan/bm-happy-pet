extends ../../templates/default
block vars
	-
		title = "Главная страница"
		breadcrumbs = [
			['Главная', 'index.html'],
			[title]
		]
block content
	.name.first-section
		.case
			.name__wrap
				img(src="uploads/happy-pet.svg" alt="")
				img(src="uploads/happy-pet-mob.svg" alt="")

	.desc
		.case
			.desc__text Лучший уровень заботы о домашнем любимце: ухаживайте за питомцем, обучайтесь, делайте покупки


	.directions
		.case
			.directions__list
				.directions__item
					.directions__item-info
						.directions__item-name
							.directions__item-title Груминг салон
							//- .directions__item-name-label
						.directions__item-text Идеальный бутик для питомца на Петровской стороне: профессиональный уход, стильная одежда и многое другое
						a.directions__item-btn.btn.--secondary.--md(href="") #[span Записать питомца]
					.directions__item-img
						img(src="uploads/direction-groom.jpg" alt="")

				.directions__item
					.directions__item-info
						.directions__item-name
							.directions__item-title Академия груминга
							.directions__item-label СКИДКА ПРИ ЗАПИСИ
						.directions__item-text Обучим профессии грумера с возможностью пройти стажировку и работать в нашем салоне
						a.directions__item-btn.btn.--primary.--md(href="") Забронировать курс
					.directions__item-img
						img(src="uploads/direction-academy.jpg" alt="")

	.we
		.case
			.we__wrap
				.we__title.s-primary #[strong Мы ]— эксперты в уходе за домашними питомцами из Санкт-Петербурга
				.we__content
					.we__text.t1 Развиваем индустрию груминга с 2011 года 1000 счастливых питомцев ежемесячно, более 500 довольных учеников академии.
					.we__btn.btn.--primary-outline.--md О нас

		.modal.--fullscreen#modal-about
			.modal__inner
				.modal__content
					.about
						.about__data
							.about__left
								.about__count
									.about__count-current 01
									.about__count-sep /
									.about__count-total
							.about__right
								.about__title #[img(src="_/uploa/" alt="")] #[span О нас]
								.about__desc Груминг центр Happy Pet— более 12 лет с любовью и заботой ухаживает за питомцами, преображая их.
								.about__text
									p Известный в Петербурге центр для животных HappyPet – комплексный бутик, в котором гармонично сочетаются груминг салон и академия, где профессионально обучают специалистов по уходу за домашними животными, магазин с первоклассными товарами для груминга и роскошный отель для домашних любимцев.
									p Запишитесь сегодня и откройте для себя комплексный уход за домашними животными под одной крышей
						.about__slider
							.about__list
								.about__item
									.about__img
										img(src="uploads/about-groom.jpg" alt="")
									.about__name Груминг салон
								.about__item
									.about__img
										img(src="uploads/about-academy.jpg" alt="")
									.about__name Академия груминга
								.about__item
									.about__img
										img(src="uploads/about-staff.jpg" alt="")
									.about__name Опытные мастера
								.about__item
									.about__img
										img(src="uploads/about-shop.jpg" alt="")
									.about__name Магазин
								.about__item
									.about__img
										img(src="uploads/about-hotel.jpg" alt="")
									.about__name Зоогостиница
			.modal__overlay

	.shares
		.case
			.shares__wrap
				.shares__slider.swiper
					.shares__slider-wrap.swiper-wrapper
						.shares__slider-item.shares__item.swiper-slide
							img(src="uploads/share-care.jpg")
						.shares__slider-item.shares__item.swiper-slide
							img(src="uploads/share-shop.jpg")
						.shares__slider-item.shares__item.swiper-slide
							img(src="uploads/share-spa.jpg")
						.shares__slider-item.shares__item.swiper-slide
							img(src="uploads/share-mc.jpg")
						.shares__slider-item.shares__item.swiper-slide
							img(src="uploads/share-test.jpg")
				.shares__controls
					.shares__control.control.--prev
						<svg width="20" height="21" viewBox="0 0 20 21" fill="none" xmlns="http://www.w3.org/2000/svg"> <path d="M11.9443 5.64404L16.8003 10.5L11.9443 15.356" stroke="currentColor" stroke-width="1.6" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/> <path d="M3.2002 10.5H16.6642" stroke="currentColor" stroke-width="1.6" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/> </svg>

					.shares__control.control.--next
