			.shares__pagination.dots
	.pros
		.case
			.pros__list
				.pros__item
					img(src="uploads/pros-installment.jpg" alt="")
				.pros__item
					img(src="uploads/pros-hotel.jpg" alt="")
				.pros__item
					img(src="uploads/pros-online-edu.png" alt="")
				.pros__item
